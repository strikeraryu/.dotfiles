{
    "description": "Option+L to start Screen Saver",
    "manipulators": [
        {
            "from": {
                "key_code": "l",
                "modifiers": {
                    "mandatory": [
                        "option",
                        "control",
                        "shift"
                    ]
                }
            },
            "to": [
                {
                    "shell_command": "open -a ScreenSaverEngine.app"
                }
            ],
            "type": "basic"
        }
    ]
}
